        <view class="weui-label">海外交流或学习时长</view>
      </view>
      <view class="weui-cell__bd">
        <picker bindchange="bindOverseaStudyTimeChange" value="{{overseaStudyTimeIndex}}" range="{{overseaStudyTime}}">
          <view class="weui-select">{{overseaStudyTime[overseaStudyTimeIndex]}}</view>
        </picker>
      </view>
    </view>

