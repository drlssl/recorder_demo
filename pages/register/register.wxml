  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell weui-cell_input">

      <view class="weui-cell__hd">
        <view class="weui-label">姓名</view>
      </view>
      <view class="weui-cell__bd">
        <input bindinput="nameInputChange" data-field="name" class="weui-input" placeholder="请输入姓名" />
      </view>
    </view>

    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
        <view class="weui-label">籍贯</view>
      </view>
      <view class="weui-cell__bd">
        <input bindinput="hometownInputChange" data-field="hometown" class="weui-input" placeholder="请输入籍贯" />
      </view>
    </view>

    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
        <view class="weui-label">英语成绩</view>
      </view>
      <view class="weui-cell__bd">
        <input bindinput="achievementInputChange" data-field="achievement" class="weui-input" type="number" placeholder="请输入高考英语成绩" />
      </view>
    </view>

    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
        <view class="weui-label">出生日期</view>
      </view>
      <view class="weui-cell__bd">
        <picker mode="date" value="2000-01-01" start="1990-01-01" end="2010-01-01" bindchange="bindDateChange">
          <view class="weui-input">{{birthday}}</view>
        </picker>
      </view>
    </view>

    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
        <view class="weui-label">性别</view>
      </view>
      <view class="weui-cell__bd">
        <picker bindchange="bindSexChange" value="{{sexIndex}}" range="{{sex}}">
          <view class="weui-select">{{sex[sexIndex]}}</view>
        </picker>
      </view>
    </view>



    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
        <view class="weui-label">利手</view>
      </view>
      <view class="weui-cell__bd">
        <picker bindchange="bindHandChange" value="{{handIndex}}" range="{{hand}}">
          <view class="weui-select">{{hand[handIndex]}}</view>
        </picker>
      </view>
    </view>

    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
        <view class="weui-label">是否英语专业</view>
      </view>
      <view class="weui-cell__bd">
        <picker bindchange="bindEnglishMajorChange" value="{{englishMajorIndex}}" range="{{englishMajor}}">
          <view class="weui-select">{{englishMajor[englishMajorIndex]}}</view>
        </picker>
      </view>
    </view>

    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
        <view class="weui-label">教育程度</view>
      </view>
      <view class="weui-cell__bd">
        <picker bindchange="bindEducationChange" value="{{educationIndex}}" range="{{education}}">
          <view class="weui-select">{{education[educationIndex]}}</view>
        </picker>
      </view>
    </view>

    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
        <view class="weui-label">学习英语时长</view>
      </view>
      <view class="weui-cell__bd">
        <picker bindchange="bindStudyEnglishTimeChange" value="{{studyEnglishTimeIndex}}" range="{{studyEnglishTime}}">
          <view class="weui-select">{{studyEnglishTime[studyEnglishTimeIndex]}}</view>
        </picker>
      </view>
    </view>


    <view class="weui-cell weui-cell_input">
      <view class="weui-cell__hd">
        <view class="weui-label">国籍</view>
      </view>
      <view class="weui-cell__bd">
        <picker bindchange="bindNationChange" value="{{nationIndex}}" range="{{nation}}">
          <view class="weui-select">{{nation[nationIndex]}}</view>
        </picker>
      </view>
    </view>

    <view class="weui-cell weui-cell_input">
